{
  "jedi.workspace.symbols.ignoreFolders": [
    "bazel-bin",
    "bazel-out",
    "bazel-source",
    "bazel-testlogs",
    "__pycache__",
    "venv"
  ],
	"languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
